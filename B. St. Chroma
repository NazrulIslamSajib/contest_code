#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    long long tt;
    cin>>tt;
    while(tt--)
    {
        long long n,x;
        cin>>n>>x;
        for(int i=0;i<x;i++)
        {
            cout<<i<<" ";
        }
        for(int i=n-1;i>=x;i--)
        {
            cout<<i<<" ";
        }
        cout<<endl;
    }
    return 0;
}
