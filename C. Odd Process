#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0);
    long long tt=1;
    cin>>tt;
    while(tt--)
    {
         long long n;
         cin>>n;
         long long arr[n+2];
         for(int i=1;i<=n;i++)
         {
             cin>>arr[i];
         }
         deque<long long>dq1,dq2;
         for(int i=1;i<=n;i++)
         {
             if(arr[i]%2)
             {
                 dq1.push_back(arr[i]);
             }
             else
             {
                 dq2.push_back(arr[i]);
             }
         }
         if(!dq1.empty())
         {
             sort(dq1.begin(),dq1.end(),greater<int>());
         }
         if(!dq2.empty())
         {
             sort(dq2.begin(),dq2.end(),greater<int>());
         }
         if(dq1.empty())
         {
             for(int i=1;i<=n;i++)
             {
                 cout<<0<<" ";
             }
             cout<<endl;
             continue;
         }
         long long num=dq1.front(),cur=-1,index=1;
         for(int i=1;i<=n;i++)
         {

              if(i==1)
              {
                  cout<<num<<" ";
                  continue;
              }
              if(!dq2.empty())
              {
                  num+=dq2.front();
                  cur=dq2.front();
                  dq2.pop_front();
                  index=i;
                  cout<<num<<" ";
              }
              else
              {
                 // cout<<endl;
                 /// cout<<i<<" "<<cur<<endl;
                  if((index-i)%2==0)
                  {
                      cout<<num<<" ";
                  }
                  else
                  {
                      if(cur==-1)
                      {
                          cout<<0<<" ";
                      }
                      else
                      {
                           if(i!=n)
                           {
                               cout<<num-cur<<" ";
                           }
                           else
                           {
                               cout<<0<<" ";
                           }
                      }
                  }
              }
         }
         cout<<endl;
    }
    return 0;
}
