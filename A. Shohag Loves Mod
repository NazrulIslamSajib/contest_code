#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    long long tt;
    cin>>tt;
    while(tt--)
    {
       long long n;
       cin>>n;
       long long x=2;
       cout<<2<<" "<<3<<" ";
       for(int i=3;i<=n;i++)
       {
           cout<<i*1+x<<" ";
           x++;
       }
       cout<<endl;
    }
    return 0;
}
